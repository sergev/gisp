#!/usr/bin/env gisp

var findFirst = `(lambda (pred xs)
    (call/cc (lambda (exit)
        (let loop ((items xs))
            (if (nullp items)
                '#f
                (if (pred (car items))
                    (exit (car items))
                    (loop (cdr items))))))))

var firstLarge = findFirst(func(n) { return n > 10; }, [1, 3, 5, 8, 13, 21])
display(firstLarge)
newline()
