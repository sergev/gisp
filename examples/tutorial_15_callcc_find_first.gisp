#!/usr/bin/env gisp

func findFirst(pred, xs) {
    return callcc(func(exit) {
        var items = xs
        while !nullp(items) {
            var value = car(items)
            if pred(value) {
                exit(value)
            }
            items = cdr(items)
        }
        return false
    })
}

var firstLarge = findFirst(func(n) { return n > 10; },
                           [1, 3, 5, 8, 13, 21])
display(firstLarge)
newline()
